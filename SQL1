Code for Question 10:

CREATE SEQUENCE emp_seq START WITH 1 INCREMENT BY 1;

SELECT emp_seq.CURRVAL, emp_seq.NEXTVAL FROM DUAL;

CREATE TABLE EMPLOYEE (

    ENO NUMERIC,
    ENAME VARCHAR(100)

);


DESCRIBE EMPLOYEE;

INSERT INTO EMPLOYEE (ENO, ENAME) VALUES (emp_seq.NEXTVAL, 'Leon');
INSERT INTO EMPLOYEE (ENO, ENAME) VALUES (emp_seq.NEXTVAL, 'Tim');
INSERT INTO EMPLOYEE (ENO, ENAME) VALUES (emp_seq.NEXTVAL, 'Dale');

SELECT * FROM EMPLOYEE;

SELECT ENO, ENAME, ROWID FROM EMPLOYEE;

DROP TABLE EMPLOYEE;

CREATE TABLE STAFF (

STAFFID NUMERIC PRIMARY KEY,
SNAME VARCHAR(100)
SSALARY NUMERIC

CONSTRAINT PK_STAFF PRIMARY KEY(STAFFID)

);


DESCRIBE STAFF;

CREATE TABLE DEPARTMENT (

    DEPARTMENTID NUMERIC PRIMARY KEY,
    DNAME VARCHAR(100)

);

ALTER TABLE STAFF ADD SDEPARTMENT NUMERIC;
ALTER TABLE STAFF ADD CONSTRAINT fk_department FOREIGN KEY
(SDEPARTMENT) REFERENCES DEPARTMENT(DEPARTMENTID);
ALTER TABLE DEPARTMENT ADD MNGR NUMERIC;
ALTER TABLE DEPARTMENT ADD CONSTRAINT fk_manager FOREIGN KEY (MNGR)
REFERENCES STAFF(STAFFID);
